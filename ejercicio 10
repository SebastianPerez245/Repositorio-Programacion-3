ejercicio 10
#include <iostream>
using namespace std;

int main(){
    int n, m;
    double A[100], B[100], R[200];

    cin >> n;
    for(int i=0; i<n; i++) cin >> A[i];

    cin >> m;
    for(int i=0; i<m; i++) cin >> B[i];

    int i=0, j=0, k=0;

    while(i<n && j<m){
        if(A[i] < B[j]) R[k++] = A[i++];
        else R[k++] = B[j++];
    }

    while(i<n) R[k++] = A[i++];
    while(j<m) R[k++] = B[j++];

    for(int x=0; x<k; x++)
        cout << R[x] << " ";

    return 0;
}
