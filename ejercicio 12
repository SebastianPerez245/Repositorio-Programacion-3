ejercicio 12
#include <iostream>
using namespace std;

int main(){
    int n;
    double v[100], temp[100];
    cin >> n;

    for(int i=0; i<n; i++)
        cin >> v[i];

    int nuevoT = 0;

    for(int i=0; i<n; i++){
        bool rep = false;
        for(int j=0; j<nuevoT; j++){
            if(v[i] == temp[j])
                rep = true;
        }
        if(!rep){
            temp[nuevoT] = v[i];
            nuevoT++;
        }
    }

    for(int i=0; i<nuevoT; i++)
        cout << temp[i] << " ";

    return 0;
}
