ejercicio 11
#include <iostream>
using namespace std;

int main(){
    int n, pos=0, neg=0, cer=0;
    double v[100];

    cin >> n;

    for(int i=0; i<n; i++){
        cin >> v[i];
        if(v[i] > 0) pos++;
        else if(v[i] < 0) neg++;
        else cer++;
    }

    cout << "Positivos: " << pos << endl;
    cout << "Negativos: " << neg << endl;
    cout << "Ceros: " << cer << endl;

    return 0;
}
