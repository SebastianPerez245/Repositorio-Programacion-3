ejercicio 13
#include <iostream>
using namespace std;

int main(){
    int n;
    double v[100];

    cin >> n;
    for(int i=0; i<n; i++) cin >> v[i];

    for(int i=0; i<n/2; i++){
        double aux = v[i];
        v[i] = v[n-1-i];
        v[n-1-i] = aux;
    }

    for(int i=0; i<n; i++)
        cout << v[i] << " ";

    return 0;
}
