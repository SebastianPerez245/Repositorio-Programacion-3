ejercicio 14
#include <iostream>
using namespace std;

int main(){
    int n;
    double v[100];

    cin >> n;
    for(int i=0; i<n; i++) cin >> v[i];

    for(int i=0; i<n-1; i++){
        for(int j=0; j<n-1-i; j++){
            if(v[j] > v[j+1]){
                double aux = v[j];
                v[j] = v[j+1];
                v[j+1] = aux;
            }
        }
    }

    for(int i=0; i<n; i++)
        cout << v[i] << " ";

    return 0;
}
