ejercicio 9
#include <iostream>
using namespace std;

int main(){
    int n, m;
    double A[100], B[100], C[200];

    cin >> n;
    for(int i=0; i<n; i++) cin >> A[i];

    cin >> m;
    for(int i=0; i<m; i++) cin >> B[i];

    for(int i=0; i<n; i++) C[i] = A[i];
    for(int i=0; i<m; i++) C[n+i] = B[i];

    for(int i=0; i<n+m; i++)
        cout << C[i] << " ";

    return 0;
}
