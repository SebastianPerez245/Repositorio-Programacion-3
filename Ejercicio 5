Ejercicio 5
#include <iostream>
using namespace std;

int main(){
    int n;
    double v[100];
    bool capicua = true;

    cin >> n;

    for(int i=0; i<n; i++)
        cin >> v[i];

    for(int i=0; i<n/2; i++){
        if(v[i] != v[n-1-i]){
            capicua = false;
            break;
        }
    }

    if(capicua) cout << "Es capicua";
    else cout << "No es capicua";

    return 0;
}
